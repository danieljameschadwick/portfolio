generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("ORM_CLIENT_URL")
}

model Card {
  id           String   @id @map("_id")
  title        String
  cardType     String
  description  String
  displayOrder Int
  subTitle     String?
  imagePath    String?
  technologies String[]
}
